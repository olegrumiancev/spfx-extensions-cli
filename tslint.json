{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      true,
      "never"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "no-consecutive-blank-lines": true,
    "ordered-imports": [
      false
    ],
    "object-literal-sort-keys": false,
    "member-access": [
      false
    ],
    "no-var-requires": false,
    "no-empty-interface": false,
    "eofline": true,
    "one-line": [
      false
    ],
    "variable-name": [
      false,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "comment-format": [
      false
    ],
    "interface-over-type-literal": false,
    "no-console": [
      false
    ],
    "max-line-length": [
      false,
      120
    ]
  },
  "rulesDirectory": []
}